<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="593.0" prefWidth="1051.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.CalendrierController">

    <ImageView fitHeight="750.0" fitWidth="1051.0" pickOnBounds="true" preserveRatio="true">
        <image>
            <Image url="@image/bg.png" />
        </image>
    </ImageView>
    <FlowPane fx:id="calendar" hgap="10.0" layoutX="221.0" layoutY="125.0" prefHeight="455.0" prefWidth="723.0" vgap="5.0" />
    <Button fx:id="backOneMonth" layoutX="443.0" layoutY="58.0" mnemonicParsing="false" onAction="#backOneMonth" text="&lt;" />
    <Text fx:id="month" layoutX="503.0" layoutY="81.0" strokeType="OUTSIDE" strokeWidth="0.0" text="####">
        <font>
            <Font size="22.0" />
        </font>
    </Text>
    <Button fx:id="forwardOneMonth" layoutX="652.0" layoutY="58.0" mnemonicParsing="false" onAction="#forwardOneMonth" text="&gt;" />
    <Text fx:id="year" layoutX="583.0" layoutY="80.0" strokeType="OUTSIDE" strokeWidth="0.0" text="####">
        <font>
            <Font size="22.0" />
        </font>
    </Text>
</AnchorPane>